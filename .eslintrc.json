// Most of these ESLint rule changes are for rapid development and should
// be modified for a long-term project
{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ],
    "no-unused-vars": "warn",
    "react/prop-types": 0,
    "jsx-a11y/control-has-associated-label": 0,
    "import/prefer-default-export": 0,
    "consistent-return": 0,
    "jsx-a11y/label-has-associated-control": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  }
}
